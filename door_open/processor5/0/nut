FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    door
    {
        type            calculated;
        value           uniform 0;
    }
    window
    {
        type            nutkWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           nonuniform List<scalar> 0();
    }
    fan
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    constraint
    {
        type            symmetry;
    }
    wall
    {
        type            nutkWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           uniform 0;
    }
    defaultFaces
    {
        type            zeroGradient;
    }
    procBoundary5to0
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary5to4
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary5to7
    {
        type            processor;
        value           uniform 0;
    }
}


