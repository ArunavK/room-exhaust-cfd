FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.01;

boundaryField
{
    door
    {
        type            kqRWallFunction;
        value           uniform 0.01;
    }
    window
    {
        type            turbulentIntensityKineticEnergyInlet;
        intensity       0.01;
        value           nonuniform List<scalar> 0();
    }
    fan
    {
        type            inletOutlet;
        inletValue      nonuniform List<scalar> 0();
        value           nonuniform List<scalar> 0();
    }
    constraint
    {
        type            symmetry;
    }
    wall
    {
        type            kqRWallFunction;
        value           uniform 0.01;
    }
    defaultFaces
    {
        type            zeroGradient;
    }
    procBoundary5to0
    {
        type            processor;
        value           uniform 0.01;
    }
    procBoundary5to4
    {
        type            processor;
        value           uniform 0.01;
    }
    procBoundary5to7
    {
        type            processor;
        value           uniform 0.01;
    }
}


