FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 83333.3333333333;

boundaryField
{
    door
    {
        type            zeroGradient;
    }
    window
    {
        type            totalPressure;
        rho             rho;
        psi             none;
        gamma           1;
        p0              nonuniform List<scalar> 0();
        value           nonuniform List<scalar> 0();
    }
    fan
    {
        type            zeroGradient;
    }
    constraint
    {
        type            symmetry;
    }
    wall
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            zeroGradient;
    }
    procBoundary3to0
    {
        type            processor;
        value           uniform 83333.3333333333;
    }
    procBoundary3to1
    {
        type            processor;
        value           uniform 83333.3333333333;
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform 83333.3333333333;
    }
}


