FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    door
    {
        type            turbulentMixingLengthFrequencyInlet;
        mixingLength    0.1;
        phi             phi;
        k               k;
        value           nonuniform List<scalar> 0();
    }
    window
    {
        type            turbulentMixingLengthFrequencyInlet;
        mixingLength    0.1;
        phi             phi;
        k               k;
        value           nonuniform List<scalar> 0();
    }
    fan
    {
        type            inletOutlet;
        inletValue      nonuniform List<scalar> 0();
        value           nonuniform List<scalar> 0();
    }
    constraint
    {
        type            symmetry;
    }
    wall
    {
        beta1           0.075;
        blended         false;
        type            omegaWallFunction;
        value           uniform 1;
    }
    defaultFaces
    {
        type            zeroGradient;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary2to4
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary2to6
    {
        type            processor;
        value           uniform 1;
    }
}


